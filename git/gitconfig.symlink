[alias]
	br = branch
	co = checkout
	st = status
[branch]	
	sort = committerdate
[color]
	ui = auto
[column]	
	ui = auto
[commit]
	gpgsign = true
	verbose = true	
[core]
	editor = code --wait
	excludesfile = ~/.gitignore_global
[credential]
	helper = osxkeychain
[diff]
	algorithm = histogram
	colorMoved = plain
	mnemonicPrefix = true
	renames = true	
	tool = vscode
[difftool "vscode"]
	cmd = code --wait --diff $LOCAL $REMOTE
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[fetch]
	prune = true
	all = true	
[filter "lfs"]
	clean = git-lfs clean -- %f
	process = git-lfs filter-process
	required = true
	smudge = git-lfs smudge -- %f
[gpg]
	format = ssh
[gpg "ssh"]
	program = /Applications/1Password.app/Contents/MacOS/op-ssh-sign
[help]
	autocorrect = prompt
[init]
	defaultBranch = master
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[push]
	autoSetupRemote = true
	default = simple
	followTags = true
[rebase]
	autoSquash = true
	autoStash = true
	updateRefs = true
[rerere]
	autoupdate = true
	enabled = true
[tag]
	sort = version:refname
[user]
	email = scott@oharagroup.net
	name = Scott O'Hara
	signingkey = ssh-rsa 
